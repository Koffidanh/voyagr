{
  "name": "voyagr",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "npx concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build",
    "seed": "node scripts/seedDB.js",
    "dev": "npm-run-all --parallel start server",
    "server": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Kayvonk/voyagr.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Kayvonk/voyagr/issues"
  },
  "homepage": "https://github.com/Kayvonk/voyagr#readme",
  "dependencies": {
    "concurrently": "^6.1.0",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.12.7",
    "nodemon": "^2.0.7"
  }
}
